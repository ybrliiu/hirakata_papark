% layout $layout;

<div class="container" id="search-<%= $search_item %>">
  <h2><%= $title %></h2>
  %= content search_form => begin
  %= end
  <div class="row">
    <h3 v-if="showResult"><%= $search_result_title %></h3>
    <div class="col s12" v-html="result"></div>
  </div>
</div>

<script>
window.addEventListener('load', function () {
  'use strict';
  hirakataPapark.searcher().<%= $search_item %>();
});
</script>
