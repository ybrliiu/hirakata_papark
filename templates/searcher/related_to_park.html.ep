% extends 'searcher/searcher';

% content search_form => begin
  <div class="row">
  % for my $name (@$check_boxes) {
    <checkbox name="<%= $search_item %>" value="<%= $name %>"></checkbox>
  % }
  </div>
  <div class="row">
    <div class="col s12 right">
      <search-button
        class="right"
        url="<%= url_for "/$lang/$url" %>"
        send-field-name="<%= $search_item %>"
      ></search-button>
    </div>
  </div>
% end

