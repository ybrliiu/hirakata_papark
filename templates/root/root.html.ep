<div class="card">
  <div id="park-map" class="card-content"></div>
</div>

<script>
window.addEventListener('load', function () {
  "use strict";
  var parkMap = new hirakataPapark.ParkMap({
    url: '<%= url_for("/$lang/park/") %>',
    mapId: 'park-map',
  });
  parkMap.registParkMarkers( JSON.parse('<%== $parks_json %>') );
});
</script>
